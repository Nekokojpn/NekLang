fn rand(i32 seed) -> i32{
	rnd := i32(0);
	rnd = seed;
	rnd = rnd * 12345;
	rnd = rnd + 1397;
	ret rnd;
};
fn main() -> i32{
	map := i32[51][51];
	i := i32(0);
	j := i32(0);
	while(i < 51){
		j = 0;
		while(j < 51){
			map[i][j] = 1;
			j = j + 1;
		};
		i = i + 1;
	};
	judge := i32(0);
	x := i32(2);
	y := i32(2);
	while(judge == 0){
		dir := i32[4];
		dir[0] = 0;
		dir[1] = 0;
		dir[2] = 0;
		dir[3] = 0;
		if(map[y][x - 1] == 1){
			if(map[y][x-2] == 1){
				dir[0] = 1;
			};
		};
		if(map[y-1][x] == 1){
			if(map[y-2][x] == 1){
				dir[1] = 1;
			};
		};
		if(map[y][x + 1] == 1){
			if(map[y][x+2] == 1){
				dir[2] = 1;
			};
		};
		if(map[y+1][x] == 1){
			if(map[y+2][x] == 1){
				dir[3] = 1;
			};
		};
		l := i32(0);
		ind := i32(0);
		while(ind < 4){
			if(dir[ind] == 1){
				l = l + 1;
			};
			ind = ind + 1;
		};
		if(l == 0){
			judge = 1;
		};
		if(judge == 0){
			writefln("OK");
		};
	};
	i = 0;
	while(i < 25){
		j = 0;
		while(j < 59){
			if(map[i][j] == 0){
				writef("*");
			};
			else{
				writef(".");
			};
			j = j + 1;
		};
		writefln(".");
		i = i + 1;
	};
	ret i;
};