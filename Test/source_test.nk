fn calc(n:i32, k:i32, h:i32[]) -> i32{
	i := i32$0;
	cnt := i32$0;
	while$ i < n{
		if$ h[i] >= k
			cnt = cnt + 1;
		i = i + 1;+
	}
	ret cnt;
}
fn main(){
	n := i32[3]{4, 1, 5};
	k := i32[3]{150, 500, 1};
	h1 := i32[4]{150, 140, 100, 200};
	h2 := i32[1]{499};
	h3 := i32[5]{100, 200, 300, 400, 500};
	writefln$ "%d", calc(n[0], k[0], h1);
	writefln$ "%d", calc(n[1], k[1], h2);
	writefln$ "%d", calc(n[2], k[2], h3);
	ret;
}