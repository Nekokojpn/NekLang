fn main() {
	target := i32[10](){1,5,8,2,3,9,4,6,7,10};
	i := i32 $ 0;
	j := i32 $ 0;
	tmp := i32 $ 0;
	while $ i < 10 {
		j = i + 1;
		while(j < 10){
			if(target[i] > target[j]){
				tmp = target[i];
				target[i] = target[j];
				target[j] = tmp;
			}
			j = j + 1;
		}
		k := i32 $ 10;
		while(k > 0){
			j = 0;
			while(j < 10){
				if(target[j] == i){
					writef $ "*";
				}
				else{
					writef $ " ";
				}
				j = j + 1;
			}
			writefln $ " ";
			k = k - 1;
		}
		i = i + 1;
	}
	ret;
}