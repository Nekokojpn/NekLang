fn display(arr : i32[10]){
	k := i32 $ 10;
	l := i32 $ 0;
	while(k > 0){
		l = 0;
		while(l < 10){
			if(arr[l] >= k){
				writef $ "*";
			}
			else{
				writef $ " ";
			}
			l = l + 1;
		}
		writefln $ " ";
		k = k - 1;
	}
	ret;
}
fn main() {
	target := i32[10](){1,5,8,4,10,9,2,6,7,3};
	i := i32 $ 0;
	j := i32 $ 0;
	tmp := i32 $ 0;
	while $ i < 10 {
		j = i + 1;
		while(j < 10){
			display(target);
			if(target[i] > target[j]){
				tmp = target[i];
				target[i] = target[j];
				target[j] = tmp;
			}
			j = j + 1;
		}
		i = i + 1;
	}
	display(target);
	ret;
}