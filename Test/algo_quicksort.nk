fn partition(a : i32[], i : i32, j : i32, x : i32){
	l : i32$ i;
	r : i32$ j;
	while(l <= r){
		while(l <= j && a[l] < x) l = l + 1;
		while(r >= i && a[r] >= x) r = r - 1;
		if(l > r) brk;
	}
}
fn quicksort(a : i32[], i : i32, j : i32){
	if(i == j)ret;
	p := i32$pivot(a, i, j);
	if(p != 0 - 1){
		k := i32$ partition(a, i, j, a[p]);
		quicksort(a, i, k-1);
		quicksort(a, k, j);
	}
	ret;
}
