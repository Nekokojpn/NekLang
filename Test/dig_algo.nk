import "io";
import "rand";
fn main(){
	a := i32[31][47];
	x := i32; y := i32; i := i32;
	for(y in 0..30)
		for(x in 0..46)
			a[y][x] = 1;
	a[1][1] = 0;
	for(i in 0..1000000-1) {
		x = (random() % 23) * 2 + 1;
		y = (random() % 15) * 2 + 1;
	}
	for(y in 0..30) {
		for(x in 0..46) {
			if(a[y][x] == 0)
				writef("  ");
			else
				writef("##");
		}
		writefln(" ");
	}
	ret;
}
