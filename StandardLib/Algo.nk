fn sort_bubble(ary: i32[], isDesc: bool) {
unsafe{
	len := i64 ary.len()-1;
	if(isDesc) {
		for(i in 0 .. len) {
			for(j in i .. len) {
				if(ary[i] < ary[j]) {
					tmp := ary[i];
					ary[i] = ary[j];
					ary[j] = tmp;
				}
			}
		}
	}
	else {
		for(i in 0 .. len) {
			for(j in i .. len) {
				if(ary[i] > ary[j]) {
					tmp := ary[i];
					ary[i] = ary[j];
					ary[j] = tmp;
				}
			}
		}
	}
	ret;}
}